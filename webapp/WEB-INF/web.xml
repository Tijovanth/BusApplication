<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>Register</servlet-name>
  <servlet-class>com.tijo.Register</servlet-class>
  <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
  <servlet-name>Register</servlet-name>
  <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>com.tijo.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Login</servlet-name>
  <url-pattern>/Login</url-pattern>
  </servlet-mapping>  
  
  <servlet>
  <servlet-name>Home</servlet-name>
  <servlet-class>com.tijo.Home</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Home</servlet-name>
  <url-pattern>/Home</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>BookTickets</servlet-name>
  <servlet-class>com.tijo.BookTickets</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>BookTickets</servlet-name>
  <url-pattern>/BookTickets</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>PrintTicket</servlet-name>
  <servlet-class>com.tijo.PrintTicket</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>PrintTicket</servlet-name>
  <url-pattern>/PrintTicket</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Logout</servlet-name>
  <servlet-class>com.tijo.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Logout</servlet-name>
  <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Admin</servlet-name>
  <servlet-class>com.tijo.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Admin</servlet-name>
  <url-pattern>/Admin</url-pattern>
  </servlet-mapping>
  
</web-app>